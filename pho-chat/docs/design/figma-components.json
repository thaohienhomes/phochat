{
  "components": {
    "Button": {
      "properties": {
        "variant": ["default","destructive","outline","secondary","ghost","link"],
        "size": ["default","sm","lg","icon"]
      },
      "defaults": { "variant": "default", "size": "default" },
      "slots": []
    },
    "SelectTrigger": {
      "properties": { "size": ["default","sm"] },
      "defaults": { "size": "default" },
      "slots": []
    },
    "DialogContent": {
      "properties": { "showCloseButton": [true, false] },
      "defaults": { "showCloseButton": true },
      "slots": ["Title","Description","Footer","Actions"]
    },
    "SheetContent": {
      "properties": { "side": ["right","left","top","bottom"] },
      "defaults": { "side": "right" },
      "slots": ["Header","Body","Footer"]
    },
    "DropdownMenuItem": {
      "properties": { "variant": ["default","destructive"], "inset": [true,false] },
      "defaults": { "variant": "default", "inset": false },
      "slots": ["Icon","Label","Shortcut"]
    },
    "TooltipProvider": {
      "properties": { "delayDuration(ms)": [0,200,400,600] },
      "defaults": { "delayDuration(ms)": 0 },
      "slots": []
    },
    "Toast": {
      "properties": { "variant": ["default","success","error"], "duration(ms)": [1200,1800,2200] },
      "defaults": { "variant": "default", "duration(ms)": 1800 },
      "slots": ["Message","Action"]
    },
    "Card": {
      "properties": {},
      "defaults": {},
      "slots": ["Header","Title","Description","Action","Content","Footer"]
    }
  },
  "_note": "Blueprint for Figma component properties matching code."
}

